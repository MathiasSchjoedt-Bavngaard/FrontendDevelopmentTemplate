<Window x:Class="fed_WPF_Eksamen.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fed_WPF_Eksamen"
        mc:Ignorable="d"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="MainWindow" Height="800" Width="800"
        Background="Transparent"
        AllowsTransparency="True"
        WindowStyle="None">

    <Border CornerRadius="400"  Background="Lavender">

        <Grid>
            <Border CornerRadius="40">
                <Button CornerRadius="200"  Content="Button" HorizontalAlignment="Left" Margin="52,172,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Width="195" Height="63">
                    <Button.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform AngleX="2.124"/>
                            <RotateTransform Angle="-44.234"/>
                            <TranslateTransform X="-0.265" Y="0.258"/>
                        </TransformGroup>
                    </Button.RenderTransform>
                </Button>
            </Border>
           

        </Grid>
    </Border>

</Window>
